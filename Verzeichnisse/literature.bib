  series     = {Communications in {Computer} and {Information} {Science}},
  title      = {Assessing {Personality} {Differences} in {Human}-{Technology} {Interaction}: {An} {Overview} of {Key} {Self}-report {Scales} to {Predict} {Successful} {Interaction}},
